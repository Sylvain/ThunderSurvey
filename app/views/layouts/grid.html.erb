<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN"
  "http://www.w3.org/TR/html4/strict.dtd">


<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>  
  <%= stylesheet_link_tag "style",'custom'  %>
  <%= stylesheet_link_tag "jquery-ui"  %>
  <%= stylesheet_link_tag "slick/slick.grid"  %>
  <%= stylesheet_link_tag "slick/slick-default-theme" %>
  <%= stylesheet_link_tag "slick/examples" %>

  <script src="/javascripts/jquery.js" type="text/javascript"></script>
  <script src="/javascripts/jquery-ui.js" type="text/javascript"></script>
  <script src="/javascripts/jrails.js" type="text/javascript"></script>
  <script src="/javascripts/jquery.scroll-to.js" type="text/javascript"></script>
  <script src="/javascripts/application.js" type="text/javascript"></script> 

  <script language="JavaScript" src="/javascripts/jquery.rule.js"></script>
  <script language="JavaScript" src="/javascripts/jquery.event.drag.js"></script>
  <script language="JavaScript" src="/javascripts/slick/slick.editors.js"></script>
  <script language="JavaScript" src="/javascripts/slick/slick.grid.js"></script>

  <%= yield :headers %>                                                   
  <title>Frontend</title>
</head>

<body>
  <div id="main_container">    
    <div class="content">
      <div id="main_content">
        <div id="flash"><%= flash_div %></div>
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>
