<!doctype html>      
<html lang="zh">
<head>
  <meta name='viewport' content='width=device-width' />
  <meta charset="utf-8" />
  <meta name="csrf-token" content="<%= form_authenticity_token %>" />
  <meta name="csrf-param" content="authenticity_token" />
  <!--[if IE]>
   <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
  <%= stylesheet_link_tag "style","jquery-ui", "ui.jqgrid", :cache => 'grid.cache'  %>
  <%= javascript_include_tag 'jquery', 'jquery-ui', 'jrails', 'rails', 'application', 
          'i18n/grid.locale-cn.js', 'jquery.jqgrid.js', :cache => 'grid.cache' %>
  
  <script type="text/javascript">
  	$.jgrid.no_legacy_api = true;
  	$.jgrid.useJSON = true;
  </script>
               
  <script type="text/javascript" charset="utf-8">
    $(document).ready(function(){     
      $('.button').button();
     })    
  </script>
  
  <%= yield :headers %>                                                   
  <title>抢座网－<%= @form.nil? ? '表单' : @form.title %></title>
</head>

<body>
 <%= render :partial => '/shared/header' %> 
      
	<div class="container clearfix">
    <%= yield %>
  </div>   
  <%#= render :partial => '/shared/footer' %>
</body>
</html>
