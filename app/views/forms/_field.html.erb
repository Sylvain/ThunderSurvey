<div class='field' id="<%= field.id %>">
  <div class="question" id="question<%= field.id %>">
    <%= label_tag field.id,field.name, :class => 'q_title'%><%=content_tag(:abbr,'*',:class=> 'required') if field.required %><br />
    <%= render :partial => "/forms/#{field.input}_field", :locals => {:field => field, :row => row} %>
    <span id="<%=field.id%>_field" class='error'></span>
  </div>
</div>
