<h1>All forms</h1>

<% @forms.each do |form| %>
  <p>
  <%= link_to form.title, form %>
  (<%= link_to 'Show', form_rows_path(form, :edit_key => form.edit_key) %> |
   <%= link_to 'Edit', edit_form_path(form, :edit_key => form.edit_key) %> |
   <%= link_to 'Destroy', form, :confirm => 'Are you sure?', :method => :delete %>
  )
  </p>
<% end %>

<p>    
  <a href="#" onclick="window.open('/forms/new', 'StatusBar', 'toolbar=yes,resizable=yes,scrollbars=no,width=980,height=900,left=0,top=0');" title="tool tip">New form</a>
</p>